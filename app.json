{
  "expo": {
    "name": "Discova",
    "slug": "Discova",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "fusia",
    "icon": "./assets/images/app.png",
    "scheme": "discova",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fusia.discova"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
       "package": "com.fusia.Discova"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "webpackConfig": "./webpack.config.js"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4083ffc9-d234-47b4-953c-813c1cf70b7a"
      }
    }
  }
}
